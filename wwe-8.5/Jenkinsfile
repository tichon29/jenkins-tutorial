pipeline {
    agent any
    tools {
        nodejs "NODEJS"
    }
    environment {
        MY_ENV_VAR = "1.0.0"
    }
    stages {
        stage('clone repo') {
            steps {
                echo "Clone the repository"
                sh '''
                    #!/bin/sh
                    hg clone https://hg.gws.genesys.com/hgweb.cgi/gws --insecure
                    echo "end"
                '''
            }
        }
    }
}